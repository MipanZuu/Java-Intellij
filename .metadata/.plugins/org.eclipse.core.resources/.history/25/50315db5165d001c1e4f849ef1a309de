          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_Uxq7gF0VEeyITIQhxyCXPg" name="run" specification="_UkjFg10VEeyITIQhxyCXPg">
            <language>java</language>
            <body>init();

		long start;
		long elapsed;
		long wait;

		while (running) {
			start = System.nanoTime();

			update();
			draw();
			drawToScreen();

			elapsed = System.nanoTime() - start;
			wait = targetTime - elapsed / 1000000;

			if (wait &lt; 0) {
				wait = 5;
			}

			try {
				Thread.sleep(wait);
			} catch (InterruptedException e) {
				e.printStackTrace();
			} catch (Exception e2) {
				e2.printStackTrace();
			}
		}
	</body>
          </ownedBehavior>
