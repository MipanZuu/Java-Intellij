            <language>java</language>
            <body>bg.draw(g2d);

		g2d.setRenderingHint(RenderingHints.KEY_TEXT_ANTIALIASING, RenderingHints.VALUE_TEXT_ANTIALIAS_ON);

		FontMetrics m;
		String text;
		int x;
		int y;


		m = g2d.getFontMetrics(levelFont);
		g2d.setFont(levelFont);

		for (int i = 0; i &lt; levelSets.length; i++) {
			if (i == currLevel) {
				g2d.setColor(Color.red);
			} else {
				g2d.setColor(Color.black);
			}

			y = makeHeight(140 + i * 27, m);

			if (i == 3) {
				y = makeHeight(140 + i * 30, m);
			}

			text = levelSets[i];
			x = makeWidth(text, m);

			g2d.drawString(text, x, y);
		}
	</body>
          </ownedBehavior>
          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_Uxul7F0VEeyITIQhxyCXPg" name="keyPressed" specification="_UlT6gF0VEeyITIQhxyCXPg">
            <ownedParameter xmi:id="_Uxul7V0VEeyITIQhxyCXPg" name="k">
              <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
            </ownedParameter>
            <language>java</language>
            <body>if (k == KeyEvent.VK_ENTER) {
			select();
		}
		if (k == KeyEvent.VK_UP) {
			currLevel--;
			if (currLevel &lt; 0) {
				currLevel = levelSets.length - 1;
			}
		}
		if (k == KeyEvent.VK_DOWN) {
			currLevel++;
			if (currLevel == levelSets.length) {
				currLevel = 0;
			}
		}

	</body>
          </ownedBehavior>
          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_UxvM8F0VEeyITIQhxyCXPg" name="select" specification="_UlVIo10VEeyITIQhxyCXPg">
