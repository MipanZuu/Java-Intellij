          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_1s54YF0TEeyITIQhxyCXPg" name="init" specification="_1cqc8F0TEeyITIQhxyCXPg">
            <language>java</language>
            <body>try {
			bg = new Background(&quot;/Backgrounds/mainbg.png&quot;, 1);
			bg.setVector(0, 0);

			font = new Font(&quot;Minecraftia&quot;, Font.PLAIN, 16);
			bgMusic = new AudioPlayer(&quot;/Music/menubgtemp.wav&quot;);
			bgMusic.play();
		} catch (Exception e) {
			e.printStackTrace();
		}
	</body>
