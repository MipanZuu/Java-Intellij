          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_1sjTFF0TEeyITIQhxyCXPg" name="run" specification="_1bIy8F0TEeyITIQhxyCXPg">
            <language>java</language>
            <body>init();

		long start;
		long elapsed;
		long wait;

		while (running) {
			start = System.nanoTime();

			update();
			draw();
			drawToScreen();

			elapsed = System.nanoTime() - start;
			wait = targetTime - elapsed / 1000000;

			if (wait &lt; 0) {
				wait = 5;
			}

			try {
				Thread.sleep(wait);
			} catch (InterruptedException e) {
				e.printStackTrace();
			} catch (Exception e2) {
				e2.printStackTrace();
			}
		}
	</body>
          </ownedBehavior>
