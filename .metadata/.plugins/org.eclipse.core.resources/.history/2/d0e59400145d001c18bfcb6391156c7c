                <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
              </ownedParameter>
              <ownedParameter xmi:id="_1s87uF0TEeyITIQhxyCXPg" name="columns">
                <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
              </ownedParameter>
              <ownedParameter xmi:id="_1s87uV0TEeyITIQhxyCXPg" type="_1c-mAF0TEeyITIQhxyCXPg" direction="return"/>
              <language>java</language>
              <body>super(sm, rows, columns);

		fillConfiguration(0, 0, getRows() - 1, getColumns() - 1, 1);
		for (int i = 0; i &lt; rows; i++) {
			for (int j = 0; j &lt; columns; j++) {
				if (i == 1 || i == 5) {
					fillConfiguration(i, j, 2);
				} else if (i == 2 || i == 4) {
					fillConfiguration(i, j, 0);
				} else if ((i == 3 &amp;&amp; (j == 0 || j == 6)) || (i == 6 &amp;&amp; (j == 0 || j == 3 || j == 6))) {
					fillConfiguration(i, j, 10);
				} else if (i == 3 &amp;&amp; (j + 1) % 2 == 0) {
					fillConfiguration(i, j, 11);
				} else if (i == 3 &amp;&amp; (j == 2 || j == 4)) {
					fillConfiguration(i, j, 3);
				}
			}
		}

		translateConfiguration();
	</body>
            </ownedBehavior>
